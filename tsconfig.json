{
	"compilerOptions": {
		"strict": true,
		"jsx": "react-native",
		"types": ["react", "react-native"],
		"allowJs": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true
	},
	"exclude": ["node_modules"],
	"extends": "expo/tsconfig.base"
}
